{
  "name": "@vslutov/redux-flux",
  "version": "0.2.2",
  "description": "Redux utils to remove boilerplate",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "test": "standard && ava",
    "build": "rollup -c",
    "clean": "rimraf lib .nyc_output",
    "cz": "git-cz",
    "cover": "nyc ava && nyc report --reporter=text-lcov | coveralls",
    "remark": "remark . .github",
    "prepublishOnly": "npm install && npm run clean && npm test && npm run build && npm prune --production && npm dedupe && npm shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vslutov/redux-flux.git"
  },
  "keywords": [
    "redux",
    "flux",
    "utils",
    "actions",
    "redux-actions"
  ],
  "author": "Vladimir Lutov <vs@lutov.net> (https://lutov.net)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/vslutov/redux-flux/issues"
  },
  "homepage": "https://github.com/vslutov/redux-flux#readme",
  "devDependencies": {
    "ava": "^2.0.0",
    "commitizen": "^3.0.7",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "esm": "^3.2.15",
    "husky": "^2.0.0",
    "nyc": "^14.0.0",
    "redux": "^4.0.1",
    "remark-cli": "^6.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-recommended": "^3.0.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "sinon": "^7.2.5",
    "standard": "^12.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "ramda": "^0.26.1",
    "redux-actions": "^2.6.4",
    "reselect": "^4.0.0"
  },
  "peerDependencies": {
    "redux": "^4.0.1"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "engines": {
    "node": ">=10.15.1",
    "npm": ">=6.4.1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
